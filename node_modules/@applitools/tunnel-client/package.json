{
  "name": "@applitools/tunnel-client",
  "version": "1.10.4",
  "homepage": "https://applitools.com",
  "bugs": {
    "url": "https://github.com/applitools/eyes.sdk.javascript1/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:applitools/eyes.sdk.javascript1.git",
    "directory": "js/packages/tunnel-client"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "Applitools Team",
    "email": "team@applitools.com"
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "default": "./dist/index.js"
    },
    "./cli": {
      "types": "./types/cli/cli.d.ts",
      "default": "./dist/cli/cli.js"
    },
    "./types": {
      "types": "./types/types.d.ts",
      "default": "./dist/types.js"
    },
    "./dist/*": {
      "types": "./types/*.d.ts",
      "default": "./dist/*.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./types/index.d.ts",
  "bin": "./dist/cli/cli.js",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "lint": "run --top-level eslint '**/*.ts'",
    "build": "yarn build:dist",
    "build:dist": "run --top-level tsc --project ./tsconfig.build.json",
    "build:bin": "yarn build && sea && yarn build:bin:zip:individual",
    "build:bin:zip": "zip -j ./bin/tunnel-client.zip $(find ./bin -type f -not -name '*.zip' -not -name '*.tar.gz' | xargs)",
    "build:bin:tgz": "tar -czf ./bin/tunnel-client.tar.gz $(find ./bin -type f -not -name '*.zip' -not -name '*.tar.gz' | xargs)",
    "build:bin:zip:individual": "find ./bin -type f -not -name '*.zip' -not -name '*.tar.gz' | xargs -I {} sh -c 'zip -j {}.zip {}'",
    "test": "run --top-level mocha './test/**/*.spec.ts' --parallel --jobs ${MOCHA_JOBS:-15} --require ./test/debug-hang.mjs",
    "test:bin": "MOCHA_GROUP=bin run --top-level mocha --parallel --jobs ${MOCHA_JOBS:-15} './test/bin/*.spec.ts' --require ./test/debug-hang.mjs",
    "test:cli": "MOCHA_GROUP=cli run --top-level mocha --parallel --jobs ${MOCHA_JOBS:-15} './test/cli/*.spec.ts' --require ./test/debug-hang.mjs",
    "test:it": "MOCHA_GROUP=it run --top-level mocha --parallel --jobs ${MOCHA_JOBS:-15} './test/it/*.spec.ts' --require ./test/debug-hang.mjs"
  },
  "sea": {
    "certs": {
      "win": {
        "cert": "env.CODE_SIGNING_CERT_WIN",
        "pass": "env.CODE_SIGNING_PASS_WIN"
      }
    },
    "targets": {
      "node20.11-linux-x64-glibc-217": "./bin/tunnel-client-linux",
      "node20-linux-x64-musl": "./bin/tunnel-client-alpine",
      "node20-linux-arm64": "./bin/tunnel-client-linux-arm64",
      "node20-darwin-x64": "./bin/tunnel-client-macos",
      "node20-win-x64": "./bin/tunnel-client-win.exe"
    }
  },
  "dependencies": {
    "@applitools/execution-grid-tunnel": "3.1.3",
    "@applitools/logger": "2.2.2",
    "@applitools/req": "1.8.2",
    "@applitools/socket": "1.3.3",
    "@applitools/utils": "1.11.1",
    "abort-controller": "3.0.0",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@applitools/sea": "^1.0.0",
    "@types/express": "^4",
    "@types/node": "^12.20.55",
    "@types/teen_process": "^2.0.4",
    "@types/yargs": "^17.0.24",
    "express": "^4.21.2",
    "nock": "^13.3.2",
    "teen_process": "^2.3.1",
    "why-is-node-running": "^3.2.2"
  },
  "engines": {
    "node": ">=12.13.0"
  },
  "publishConfig": {
    "access": "public"
  }
}